.btn {
  width: 100vw;
  position: fixed;
  bottom: 3vh;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  height: 11vh;
}

.btn-item {
  width: 90vw;
  font-size: 32rpx;
  color: white;
  background:rgb(64, 107, 181);
  height: 5vh;
  align-items: center;
  justify-content: center;
  border-radius: 12rpx;
  line-height: 5vh;
}

.data {
  background-color: rgba(255,255,255,0.8);
  height: 14vh;
  margin: 69vh 5vw 0 5vw;
  border-radius: 24rpx;
  padding: 0 5vw;
  display: flex;
  align-items: right;
  justify-content: center;
  flex-direction: column;
}

.data cover-image {
  position: absolute;
  right: 10vw;
  width: 7vh;
  height: 7vh;
}
.detail {
  font-size: 2.2vh;
  font-weight: bold;
  color: var(--fontcolor);
  margin-top: 1vh;
}

.title {
  font-size: 3vh;
  color: var(--fontcolor);
  font-weight: bold;
}
